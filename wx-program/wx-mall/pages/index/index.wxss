.index_swiper swiper {
  height: 340rpx;
}

.index_swiper image {
  width: 100%;
}

.index_cate {
  display: flex;
}

.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}

.index_cate navigator image {
  width: 100%;
}

.index_floor .floor_title {
  padding: 10rpx 0;
}

.index_floor .floor_title image {
  width: 100%;
}

.index_floor .floor_list {
  overflow: hidden;
  border-left: 10rpx solid #fff;
  border-right: 10rpx solid #fff;
}

.index_floor .floor_list navigator {
  float: left;
  width: 33.33%;
}

/* 后4个链接 */
.index_floor .floor_list navigator:nth-last-child(-n+4) {
  height: 27.7vw;
  border-left: 10rpx solid #fff;
}

/* 第2、3个链接 */
.index_floor .floor_list navigator:nth-child(2), navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}

.index_floor .floor_list navigator image {
  width: 100%;
  height: 100%;
}

